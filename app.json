{
  "name": "Pygds",
  "description": "Pygds package",
  "image": "heroku/python",
  "repository": "https://github.com/cosmopolitan-travel-serivce/pygds",
  "keywords": ["python"],
  "addons": [],
  "env": {
    "SECRET_KEY": {
      "description": "The secret key for the pygds package.",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test-setup": "pip install nose coverage flake8",
        "test": "nosetests --with-coverage --cover-package=pygds --cover-min-percentage=40 --cover-tests"
      }
    }
  }
}
